@model IEnumerable<AssetManagementSystem.Models.Asset>;
@{
    ViewData["Title"] = "AddAsset";
}
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Year</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Year</td>
        </tr>
    }
</table>